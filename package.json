{
  "name": "datav-vue",
  "version": "0.0.1",
  "description": "A Powerful Data Visualization Tool",
  "scripts": {
    "new": "plop --plopfile ./plop-templates/plopfile.js",
    "bootstrap": "pnpm install --frozen-lockfile",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true vite build",
    "serve": "vite preview",
    "lint": "pnpm run lint:type && pnpm run lint:code && pnpm run lint:css",
    "lint:type": "vue-tsc --noEmit",
    "lint:code": "eslint 'src/**/*.{js,ts,vue}'",
    "lint:css": "stylelint 'src/**/*.(css|scss|vue)'",
    "lint:fix": "pnpm run lint:fix-code && pnpm run lint:fix-css",
    "lint:fix-code": "eslint --fix 'src/**/*.{js,ts,vue}'",
    "lint:fix-css": "stylelint --fix 'src/**/*.(css|scss|vue)'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install"
  },
  "dependencies": {
    "@monaco-editor/loader": "1.3.2",
    "@vueuse/core": "^9.2.0",
    "accounting": "~0.4.1",
    "axios": "~0.27.2",
    "axios-mock-adapter": "~1.21.2",
    "crypto-js": "~4.1.1",
    "dayjs": "~1.11.5",
    "echarts": "^5.3.3",
    "echarts-wordcloud": "^2.0.0",
    "element-plus": "2.2.17",
    "gsap": "~3.11.1",
    "html2canvas": "^1.4.1",
    "js-cookie": "~3.0.1",
    "lodash-es": "~4.17.21",
    "mockjs": "~1.1.0",
    "monaco-editor": "0.34.0",
    "naive-ui": "^2.33.3",
    "number-precision": "~1.5.2",
    "particles.vue3": "~2.3.2",
    "pinia": "^2.0.22",
    "shortid": "~2.2.16",
    "tsparticles": "^2.3.2",
    "vue": "^3.2.39",
    "vue-echarts": "^6.2.3",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "~7.19.1",
    "@babel/preset-env": "~7.19.1",
    "@babel/preset-typescript": "~7.18.6",
    "@commitlint/cli": "~17.1.2",
    "@commitlint/config-conventional": "~17.1.0",
    "@types/accounting": "~0.4.2",
    "@types/crypto-js": "~4.1.1",
    "@types/lodash-es": "~4.17.6",
    "@types/mockjs": "~1.0.6",
    "@types/shortid": "~0.0.29",
    "@typescript-eslint/eslint-plugin": "~5.38.0",
    "@typescript-eslint/parser": "~5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/component-compiler-utils": "^3.3.0",
    "@vue/runtime-core": "^3.2.39",
    "conventional-changelog-cli": "~2.2.2",
    "cross-env": "~7.0.3",
    "eslint": "~8.23.1",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-vue": "~9.5.1",
    "handlebars": "~4.7.7",
    "husky": "~8.0.1",
    "lint-staged": "~13.0.3",
    "plop": "~3.1.1",
    "postcss": "~8.4.16",
    "postcss-html": "~1.5.0",
    "postcss-scss": "~4.0.5",
    "rimraf": "~3.0.2",
    "rollup-plugin-visualizer": "^5.8.1",
    "sass": "~1.55.0",
    "stylelint": "~14.12.1",
    "stylelint-config-prettier": "~9.0.3",
    "stylelint-config-standard": "~28.0.0",
    "tslib": "~2.4.0",
    "typescript": "~4.8.3",
    "unocss": "^0.45.22",
    "unplugin-element-plus": "0.4.1",
    "vite": "^3.1.3",
    "vite-plugin-plain-text": "^1.2.1",
    "vue-tsc": "^0.40.13"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pengxiaotian/datav-vue.git"
  },
  "homepage": "https://datav.pengxiaotian.com",
  "author": "Xiaotian (Allen) Peng",
  "license": "MIT",
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not IE 11"
  ]
}
